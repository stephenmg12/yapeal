{
    "name":              "yapeal/yapeal",
    "description":       "Yet Another Php Eve Api Library",
    "license":           "LGPL-3.0+",
    "authors":           [
        {
            "name":  "Michael Cummings",
            "email": "dragonaire@users.sourceforge.net",
            "role":  "Developer"
        },
        {
            "name":  "Somerset Mahm",
            "email": "somersetmahm@users.sourceforge.net",
            "role":  "Developer"
        }
    ],
    "homepage":          "https://sourceforge.net/p/yapeal/wiki/Home/",
    "type":              "project",
    "support":           {
        "issues": "https://sourceforge.net/p/yapeal/tickets/",
        "source": "https://sourceforge.net/projects/yapeal/files/",
        "wiki":   "https://sourceforge.net/p/yapeal/wiki/Home/"
    },
    "require":           {
        "doctrine/dbal":                "2.4.*@dev",
        "doctrine/migrations":          "dev-master",
        "doctrine/orm":                 "2.4.*@dev",
        "ext-apc":                      "*",
        "ext-curl":                     "*",
        "ext-date":                     "*",
        "ext-hash":                     "*",
        "ext-mysqli":                   "*",
        "ext-openssl":                  "*",
        "ext-PDO":                      "*",
        "ext-pdo_mysql":                "*",
        "ext-Reflection":               "*",
        "ext-SPL":                      "*",
        "ext-xmlreader":                "*",
        "guzzle/guzzle":                "3.7.*@dev",
        "lib-curl":                     ">=7.19.7",
        "lib-openssl":                  ">=0.9.8",
        "monolog/monolog":              "1.6.*@dev",
        "php":                          ">=5.3.6",
        "symfony/config":               "2.4.*@dev",
        "symfony/console":              "2.4.*@dev",
        "symfony/dependency-injection": "2.4.*@dev",
        "symfony/event-dispatcher":     "2.4.*@dev",
        "symfony/filesystem":           "2.4.*@dev",
        "symfony/yaml":                 "2.4.*@dev"
    },
    "require-dev":       {
        "behat/behat":                 "2.5.*@dev",
        "ext-xsl":                     "*",
        "phpdocumentor/phpdocumentor": "dev-develop",
        "phpunit/phpunit":             "3.7.*@dev",
        "phpunit/dbunit":              ">=1.2",
        "phpunit/php-invoker":         "*",
        "twig/twig":                   "v1.13.2"
    },
    "autoload":          {
        "psr-0": {
            "Yapeal\\": "src/",
            "":         "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable":     true
}
