{
    "name": "yapeal/yapeal",
    "description": "Yet Another Php Eve Api Library also know as Yapeal which can be used to access the Eve Online API data and place it into a database.",
    "keywords": ["curl", "database", "Eve", "Eve-Online", "PDO", "REST", "RESTful"],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Michael Cummings",
            "email": "dragonaire@users.sourceforge.net",
            "role": "Developer"
        },
        {
            "name": "Somerset Mahm",
            "email": "somersetmahm@users.sourceforge.net",
            "role": "Developer"
        }
    ],
    "homepage": "https://sourceforge.net/p/yapeal/wiki/Home/",
    "support": {
        "issues": "https://sourceforge.net/p/yapeal/tickets/",
        "source": "https://sourceforge.net/projects/yapeal/files/",
        "wiki": "https://sourceforge.net/p/yapeal/wiki/Home/"
    },
    "require": {
        "doctrine/dbal": "2.4.*@dev",
        "doctrine/migrations": "dev-master",
        "doctrine/orm": "2.4.*@dev",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-hash": "*",
        "ext-mysqli": "*",
        "ext-openssl": "*",
        "ext-PDO": "*",
        "ext-pdo_mysql": "*",
        "ext-Reflection": "*",
        "ext-SPL": "*",
        "ext-xmlreader": "*",
        "guzzle/guzzle": "3.7.*@dev",
        "lib-curl": ">=7.19.7",
        "lib-openssl": ">=0.9.8",
        "monolog/monolog": "1.6.*@dev",
        "php": ">=5.3.6",
        "pimple/pimple": "2.0.*@dev",
        "symfony/config": "2.5.*@dev"
    },
    "require-dev": {
        "mikey179/vfsStream": "v1.2.0",
        "phpmd/phpmd": "1.5.0",
        "phpunit/phpunit": "3.7.*@dev",
        "phpunit/dbunit": ">=1.2",
        "sebastian/phpcpd": "1.4.x-dev",
        "squizlabs/php_codesniffer": "1.4.*",
        "twig/twig": "v1.13.2"
    },
    "autoload": {
        "psr-4": {
            "Yapeal\\": ["lib/", "bin/"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yapeal\\Tests\\": ["tests/"]
        }
    },
    "bin": ["bin/yapeal-cli.php"],
    "minimum-stability": "dev",
    "prefer-stable": true
}
