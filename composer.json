{
    "name":              "yapeal/yapeal",
    "description":       "Yet Another Php Eve Api Library",
    "license":           "LGPL-3.0+",
    "authors":           [
        {
            "name":  "Michael Cummings",
            "email": "dragonaire@users.sourceforge.net",
            "role":  "Developer"
        },
        {
            "name":  "Somerset Mahm",
            "email": "somersetmahm@users.sourceforge.net",
            "role":  "Developer"
        }
    ],
    "homepage":          "https://sourceforge.net/p/yapeal/wiki/Home/",
    "support":           {
        "issues": "https://sourceforge.net/p/yapeal/tickets/",
        "source": "https://sourceforge.net/projects/yapeal/files/",
        "wiki":   "https://sourceforge.net/p/yapeal/wiki/Home/"
    },
    "require":           {
        "doctrine/dbal":                "2.4.*@dev",
        "doctrine/migrations":          "dev-master",
        "doctrine/orm":                 "2.4.*@dev",
        "ext-apc":                      "*",
        "ext-curl":                     "*",
        "ext-date":                     "*",
        "ext-hash":                     "*",
        "ext-mysqli":                   "*",
        "ext-openssl":                  "*",
        "ext-PDO":                      "*",
        "ext-pdo_mysql":                "*",
        "ext-Reflection":               "*",
        "ext-SPL":                      "*",
        "ext-xmlreader":                "*",
        "guzzle/guzzle":                "3.7.*@dev",
        "lib-curl":                     ">=7.19.7",
        "lib-openssl":                  ">=0.9.8",
        "monolog/monolog":              "1.6.*@dev",
        "php":                          ">=5.3.6",
        "symfony/config":               "2.4.*@dev",
        "symfony/console":              "2.4.*@dev",
        "symfony/dependency-injection": "2.4.*@dev",
        "symfony/event-dispatcher":     "2.4.*@dev",
        "symfony/filesystem":           "2.4.*@dev",
        "symfony/yaml":                 "2.4.*@dev",
        "phpmd/phpmd":                  "1.5.0",
        "sebastian/phpcpd":             "1.4.x-dev"
    },
    "require-dev":       {
        "behat/behat":                               "2.5.*@dev",
        "ext-xsl":                                   "*",
        "mikey179/vfsStream":                        "v1.2.0",
        "phpdocumentor/template-abstract":           "1.2.1",
        "phpdocumentor/template-clean":              "1.0.1",
        "phpdocumentor/template-responsive-twig":    "1.2.1",
        "phpdocumentor/template-responsive":         "1.3.1",
        "phpdocumentor/template-new-black":          "1.3.1",
        "phpdocumentor/template-zend":               "1.3.1",
        "phpdocumentor/template-old-ocean":          "1.3.1",
        "phpdocumentor/template-checkstyle":         "1.2.0",
        "phpdocumentor/template-xml":                "1.2.0",
        "phpdocumentor/graphviz":                    "1.0.0",
        "phpdocumentor/phpdocumentor":               "v2.1.0",
        "phpunit/phpunit":                           "3.7.*@dev",
        "phpunit/dbunit":                            ">=1.2",
        "phpunit/php-invoker":                       "*",
        "squizlabs/php_codesniffer":                 "1.4.*",
        "twig/twig":                                 "v1.13.2",
        "whatthejeff/nyancat-phpunit-resultprinter": "1.1.*@dev"
    },
    "autoload":          {
        "classmap": ["Yapeal/"]
    },
    "bin":               ["yapeal-cli.php"],
    "minimum-stability": "dev",
    "prefer-stable":     true
}
