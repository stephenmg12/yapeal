{
    "name": "yapeal/yapeal",
    "description": "Yet Another Php Eve Api Library also know as Yapeal which can be used to access the Eve Online API data and place it into a database.",
    "keywords": ["curl", "database", "Eve", "Eve-Online", "PDO", "REST", "RESTful"],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Michael Cummings",
            "email": "dragonaire@users.sourceforge.net",
            "role": "Developer"
        },
        {
            "name": "Somerset Mahm",
            "email": "somersetmahm@users.sourceforge.net",
            "role": "Developer"
        }
    ],
    "homepage": "https://sourceforge.net/p/yapeal/wiki/Home/",
    "support": {
        "issues": "https://sourceforge.net/p/yapeal/tickets/",
        "source": "https://sourceforge.net/projects/yapeal/files/",
        "wiki": "https://sourceforge.net/p/yapeal/wiki/Home/"
    },
    "require": {
        "doctrine/dbal": "~2.4",
        "doctrine/migrations": "1.0.*@dev",
        "doctrine/orm": "~2.4",
        "ext-curl": "*",
        "ext-date": "*",
        "ext-hash": "*",
        "ext-mysqli": "*",
        "ext-openssl": "*",
        "ext-PDO": "*",
        "ext-pdo_mysql": "*",
        "ext-Reflection": "*",
        "ext-SPL": "*",
        "ext-xmlreader": "*",
        "guzzle/guzzle": "~3.8",
        "lib-curl": ">=7.19.7",
        "lib-openssl": ">=0.9.8",
        "monolog/monolog": "~1.8",
        "php": ">=5.4.0",
        "symfony/config": "~2.4"
    },
    "require-dev": {
        "mikey179/vfsStream": "v1.2.0",
        "phpmd/phpmd": "~1.5",
        "phpunit/phpunit": "~4.0",
        "phpunit/dbunit": "~1.3",
        "sebastian/phpcpd": "~2.0",
        "squizlabs/php_codesniffer": "~1.5",
        "twig/twig": "v1.13.2"
    },
    "autoload": {
        "psr-4": {
            "Yapeal\\": ["lib/", "bin/", "tests/"]
        }
    },
    "bin": ["bin/yapeal-cli.php"],
    "minimum-stability": "dev",
    "prefer-stable": true
}
