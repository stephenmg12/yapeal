{
    "yapeal": {
        "application_agent": "",
        "registered_mode": "optional",
        "Cache": {
            "interval": 7,
            "to": "file",
            "dir": "{libraryBase}/cache"
        },
        "Database": {
            "user": "YapealUser",
            "host": "localhost",
            "password": "secret",
            "database": "yapeal",
            "prefix": "mysql"
        },
        "Logging": {
            "locale": "en_US",
            "level": 123,
            "dir": "{libraryBase}/logs"
        }
    }
}
