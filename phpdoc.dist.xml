<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <files>
        <directory>./Yapeal</directory>
        <ignore>docs/*</ignore>
        <ignore>junk.php</ignore>
        <ignore>test.php</ignore>
        <ignore>Tests/*</ignore>
        <ignore>vendor/*</ignore>
        <ignore>yapeal-cli.php</ignore>
        <ignore>bootstrap.php</ignore>
        <ignore>cli-config.php</ignore>
    </files>
    <parser default-package-name="Yapeal 2">
        <target>Yapeal/docs/cache</target>
    </parser>
    <title>Yapeal 2</title>
    <transformations>
        <template name="new-black"/>
    </transformations>
    <transformer>
        <target>Yapeal/docs/html</target>
    </transformer>
</phpdoc>
