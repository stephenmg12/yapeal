<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <files>
        <directory>src/Yapeal</directory>
        <ignore>src/Yapeal/docs/*</ignore>
        <ignore>junk.php</ignore>
        <ignore>test.php</ignore>
        <ignore>Yapeal/Tests/*</ignore>
        <ignore>Yapeal/vendor/*</ignore>
        <ignore>yapeal-cli.php</ignore>
        <ignore>Yapeal/bootstrap.php</ignore>
        <ignore>Yapeal/cli-config.php</ignore>
    </files>
    <parser default-package-name="Yapeal 2">
        <target>src/Yapeal/docs/cache</target>
    </parser>
    <title>Yapeal 2</title>
    <transformations>
        <template name="new-black"/>
    </transformations>
    <transformer>
        <target>src/Yapeal/docs/html</target>
    </transformer>
</phpdoc>
